{
  "name": "vprof-frontend",
  "author": "nvdv",
  "main": "vprof/frontend/main.js",
  "dependencies": {
    "d3-array": "1.0.2",
    "d3-axis": "1.0.3",
    "d3-format": "1.0.2",
    "d3-hierarchy": "1.0.3",
    "d3-request": "1.0.3",
    "d3-scale": "1.0.3",
    "d3-shape": "1.0.3",
    "d3-selection": "1.0.3",
    "eslint": "latest",
    "uglify-js": "latest",
    "browserify": "12",
    "browserify-css": "latest",
    "karma": "latest",
    "karma-jasmine": "latest",
    "karma-phantomjs-launcher": "latest",
    "karma-browserify": "5.0.0",
    "phantomjs-prebuilt": "2.1.3",
    "jasmine-core": "latest",
    "highlight.js": "latest",
    "watchify": "3.7.0"
  },
  "scripts": {
    "lint": "eslint vprof/frontend",
    "build": "browserify -g browserify-css vprof/frontend/main.js | uglifyjs > vprof/frontend/vprof_min.js",
    "watch": "watchify vprof/frontend/main.js -o vprof/frontend/vprof_min.js -v",
    "test": "karma start"
  },
  "browserify": {
      "transform": ["browserify-css"]
